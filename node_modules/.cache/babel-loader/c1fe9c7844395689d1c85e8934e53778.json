,KAAK,CAAC;QAC3CI,QAAQ,CAACC,cAAc,CAAC,UAAU,CAAC,CAACC,WAAW,CAACC,OAAO,CAAC;QACxD,IAAI,CAACb,aAAa,EAAE;MACtB;MACA,IAAI,CAACD,UAAU,EAAE;IACnB,CAAC,CAAC;EACJ,CAAC;EACDe,OAAO,EAAE;IACPL,KAAK,CAACH,IAAI,EAAE;MACV,OAAOA,IAAI,CAACS,OAAM,KAAM,QAAQ;IAClC,CAAC;IAEDP,WAAW,CAACF,IAAI,EAAE;MAChB,IAAIU,GAAE,GAAIN,QAAQ,CAACO,aAAa,CAAC,KAAK,CAAC;QACrCC,IAAIR,QAAQ,CAACO,aAAa,CAAC,GAAG,CAAC;MACjCD,GAAG,CAACG,SAAQ,GAAI,4BAA4B;MAC5CD,CAAC,CAACC,SAAQ,GAAI,UAAS,GAAIb,IAAI,CAACf,IAAI;MACpCyB,GAAG,CAACJ,WAAW,CAACM,CAAC,CAAC;MAClB,IAAIE,IAAG,GAAIV,QAAQ,CAACO,aAAa,CAAC,MAAM,CAAC;MACzCG,IAAI,CAACR,WAAW,CAACF,QAAQ,CAACW,cAAc,CAAC,MAAK,GAAIf,IAAI,CAACf,IAAI,CAAC,CAAC;MAC7DyB,GAAG,CAACJ,WAAW,CAACQ,IAAI,CAAC;MACrB,OAAOJ,GAAG;IACZ,CAAC;IAEDM,YAAY,CAAChB,IAAI,EAAE;MACjB,OAAO,OAAOA,IAAI,CAACiB,UAAS,IAAK,WAAU,GAAI,KAAI,GAAIjB,IAAI,CAACiB,UAAU;IACxE;EACF;AACF,CAAC","names":["Layout","PageHeader","appConfig","icons","page","title","meta","name","content","description","data","items","text","href","active","iconsCount","newIconsCount","components","mounted","push","hex","forEach","icon","item","getIconItem","isNew","document","getElementById","appendChild","newItem","methods","version","div","createElement","i","className","span","createTextNode","isDeprecated","deprecated"],"sourceRoot":"","sources":["C:\\Apps\\Velson\\modern\\src\\views\\icons\\materialdesign.vue"],"sourcesContent":["<script>\nimport Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport appConfig from \"../../../app.config\";\n\nimport { icons } from \"./data-material\";\n\nexport default {\n  page: {\n    title: \"Material Design\",\n    meta: [{ name: \"description\", content: appConfig.description }],\n  },\n  data() {\n    return {\n      icons: icons,\n      title: \"Material Design\",\n      items: [\n        {\n          text: \"Icons\",\n          href: \"/\",\n        },\n        {\n          text: \"Material Design\",\n          active: true,\n        },\n      ],\n      iconsCount: 0,\n      newIconsCount: 0,\n    };\n  },\n  components: {\n    Layout,\n    PageHeader,\n  },\n  mounted() {\n    this.icons.push({ name: \"blank\", hex: \"f68c\" });\n\n    this.icons.forEach((icon) => {\n      var item = this.getIconItem(icon, this.isNew(icon));\n      document.getElementById(\"icons\").appendChild(item);\n      if (this.isNew(icon)) {\n        var newItem = this.getIconItem(icon, false);\n        document.getElementById(\"newIcons\").appendChild(newItem);\n        this.newIconsCount++;\n      }\n      this.iconsCount++;\n    });\n  },\n  methods: {\n    isNew(icon) {\n      return icon.version === \"5.0.45\";\n    },\n\n    getIconItem(icon) {\n      var div = document.createElement(\"div\"),\n        i = document.createElement(\"i\");\n      div.className = \"col-xl-3 col-lg-4 col-sm-6\";\n      i.className = \"mdi mdi-\" + icon.name;\n      div.appendChild(i);\n      var span = document.createElement(\"span\");\n      span.appendChild(document.createTextNode(\"mdi-\" + icon.name));\n      div.appendChild(span);\n      return div;\n    },\n\n    isDeprecated(icon) {\n      return typeof icon.deprecated == \"undefined\" ? false : icon.deprecated;\n    },\n  },\n};\n</script>\n\n<template>\n  <Layout>\n    <PageHeader :title=\"title\" :items=\"items\" />\n    <div class=\"row icon-demo-content\">\n      <div class=\"col-12\">\n        <div class=\"card\">\n          <div class=\"card-body\">\n            <h4 class=\"card-title\">New Icons</h4>\n            <p class=\"card-title-desc mb-2\">\n              Use\n              <code>&lt;i class=\"mdi mdi-*-*\"&gt;&lt;/i&gt;</code> class.<span\n                class=\"badge bg-success\"\n                >v 6.5.95</span\n              >.\n            </p>\n\n            <div class=\"row icon-demo-content\" id=\"newIcons\"></div>\n          </div>\n          <!-- end card-body -->\n        </div>\n        <!-- end card -->\n\n        <div class=\"card\">\n          <div class=\"card-body\">\n            <h4 class=\"card-title mb-4\">All Icons</h4>\n            <div class=\"row icon-demo-content\" id=\"icons\"></div>\n          </div>\n          <!-- end card-body -->\n        </div>\n        <!-- end card -->\n      </div>\n      <!-- end col -->\n    </div>\n    <!-- end row -->\n\n    <div class=\"row\">\n      <div class=\"col-12\">\n        <div class=\"card\">\n          <div class=\"card-body\">\n            <h4 class=\"card-title\">Size</h4>\n\n            <div class=\"row icon-demo-content\">\n              <div class=\"col-xl-3 col-lg-4 col-sm-6\">\n                <i class=\"mdi mdi-18px mdi-account\"></i> mdi-18px\n              </div>\n\n              <div class=\"col-xl-3 col-lg-4 col-sm-6\">\n                <i class=\"mdi mdi-24px mdi-account\"></i> mdi-24px\n              </div>\n\n              <div class=\"col-xl-3 col-lg-4 col-sm-6\">\n                <i class=\"mdi mdi-36px mdi-account\"></i> mdi-36px\n              </div>\n\n              <div class=\"col-xl-3 col-lg-4 col-sm-6\">\n                <i class=\"mdi mdi-48px mdi-account\"></i> mdi-48px\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- end row -->\n\n    <div class=\"row\">\n      <div class=\"col-12\">\n        <div class=\"card\">\n          <div class=\"card-body\">\n            <h4 class=\"card-title\">Rotate</h4>\n\n            <div class=\"row icon-demo-content\">\n              <div class=\"col-xl-3 col-lg-4 col-sm-6\">\n                <i class=\"mdi mdi-rotate-45 mdi-account\"></i> mdi-rotate-45\n              </div>\n              <div class=\"col-xl-3 col-lg-4 col-sm-6\">\n                <i class=\"mdi mdi-rotate-90 mdi-account\"></i> mdi-rotate-90\n              </div>\n              <div class=\"col-xl-3 col-lg-4 col-sm-6\">\n                <i class=\"mdi mdi-rotate-135 mdi-account\"></i> mdi-rotate-135\n              </div>\n              <div class=\"col-xl-3 col-lg-4 col-sm-6\">\n                <i class=\"mdi mdi-rotate-180 mdi-account\"></i> mdi-rotate-180\n              </div>\n              <div class=\"col-xl-3 col-lg-4 col-sm-6\">\n                <i class=\"mdi mdi-rotate-225 mdi-account\"></i> mdi-rotate-225\n              </div>\n              <div class=\"col-xl-3 col-lg-4 col-sm-6\">\n                <i class=\"mdi mdi-rotate-270 mdi-account\"></i> mdi-rotate-270\n              </div>\n              <div class=\"col-xl-3 col-lg-4 col-sm-6\">\n                <i class=\"mdi mdi-rotate-315 mdi-account\"></i> mdi-rotate-315\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- end row -->\n\n    <div class=\"row\">\n      <div class=\"col-12\">\n        <div class=\"card\">\n          <div class=\"card-body\">\n            <h4 class=\"card-title\">Spin</h4>\n\n            <div class=\"row icon-demo-content\">\n              <div class=\"col-xl-3 col-lg-4 col-sm-6\">\n                <i class=\"mdi mdi-spin mdi-loading\"></i> mdi-spin\n              </div>\n              <div class=\"col-xl-3 col-lg-4 col-sm-6\">\n                <i class=\"mdi mdi-spin mdi-star\"></i> mdi-spin\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- end row -->\n  </Layout>\n</template>\n"]}]}