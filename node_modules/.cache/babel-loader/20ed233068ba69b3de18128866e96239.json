adeIntensity: 1,\n            inverseColors: false,\n            opacityFrom: 0.45,\n            opacityTo: 0.05,\n            stops: [50, 100, 100, 100],\n          },\n        },\n        colors: [\"#405189\"],\n      },\n    };\n  },\n  components: {\n    Layout,\n    PageHeader,\n    Portfolio,\n    MarketGraph,\n    RecentActivity,\n    TopProgram,\n    News,\n    Currencies,\n    Trading,\n    Swiper,\n    SwiperSlide,\n  },\n};\n</script>\n\n<template>\n  <Layout>\n    <PageHeader :title=\"title\" :items=\"items\" />\n\n    <div class=\"row\">\n      <div class=\"col-xxl-3\">\n        <Portfolio />\n      </div>\n      <!-- end col -->\n\n      <div class=\"col-xxl-9 order-xxl-0 order-first\">\n        <div class=\"row\">\n          <div\n            class=\"col-lg-4 col-md-6\"\n            v-for=\"(item, index) of cyptoWidgets\"\n            :key=\"index\"\n          >\n            <div class=\"card\">\n              <div class=\"card-body\">\n                <div class=\"d-flex align-items-center\">\n                  <div class=\"avatar-sm flex-shrink-0\">\n                    <span\n                      class=\"\n                        avatar-title\n                        bg-light\n                        text-primary\n                        rounded-circle\n                        fs-3\n                      \"\n                    >\n                      <i :class=\"`${item.icon} align-middle`\"></i>\n                    </span>\n                  </div>\n                  <div class=\"flex-grow-1 ms-3\">\n                    <p class=\"text-uppercase fw-semibold fs-12 text-muted mb-1\">\n                      {{ item.label }}\n                    </p>\n                    <h4 class=\"mb-0\">\n                      $\n                      <span class=\"counter-value\">\n                        {{ item.counter }}\n                      </span>\n                    </h4>\n                  </div>\n                  <div class=\"flex-shrink-0 align-self-end\">\n                    <span :class=\"`badge badge-soft-${item.badgeColor}`\">\n                      <i :class=\"`${item.badge} align-middle me-1`\"></i>\n                      {{ item.percentage }} %\n                      <span></span>\n                    </span>\n                  </div>\n                </div>\n              </div>\n              <!-- end card body -->\n            </div>\n            <!-- end card -->\n          </div>\n          <!-- end col -->\n        </div>\n        <!-- end row -->\n\n        <div class=\"row\">\n          <div class=\"col-xl-12\">\n            <MarketGraph />\n            <!-- end card -->\n          </div>\n          <!-- end col -->\n        </div>\n        <!-- end row -->\n      </div>\n      <!-- end col -->\n    </div>\n    <!-- end row -->\n\n    <div class=\"row\">\n      <div class=\"col-lg-12\">\n        <swiper\n          class=\"cryptoSlider\"\n          :slidesPerView=\"1\"\n          :loop=\"false\"\n          :spaceBetween=\"24\"\n          :navigation=\"{\n            nextEl: '.swiper-button-next',\n            prevEl: '.swiper-button-prev',\n          }\"\n          :autoplay=\"{\n            delay: 2500,\n            disableOnInteraction: false,\n          }\"\n          :breakpoints=\"{\n            640: {\n              slidesPerView: 2,\n            },\n            768: {\n              slidesPerView: 2.5,\n            },\n            1024: {\n              slidesPerView: 3,\n            },\n            1200: {\n              slidesPerView: 5,\n            },\n          }\"\n        >\n          <template v-for=\"(item, index) of cryptoSlider\" :key=\"index\">\n            <swiper-slide>\n              <div class=\"card\">\n                <div class=\"card-body\">\n                  <div class=\"float-end\">\n                    <div class=\"dropdown\">\n                      <a\n                        class=\"text-reset\"\n                        href=\"#\"\n                        data-bs-toggle=\"dropdown\"\n                        aria-haspopup=\"true\"\n                        aria-expanded=\"false\"\n                      >\n                        <span class=\"text-muted fs-18\">\n                          <i class=\"mdi mdi-dots-horizontal\"></i>\n                        </span>\n                      </a>\n                      <div class=\"dropdown-menu dropdown-menu-end\">\n                        <a class=\"dropdown-item\" href=\"#\">Details</a>\n                        <a class=\"dropdown-item\" href=\"#\">Cancel</a>\n                      </div>\n                    </div>\n                  </div>\n                  <div class=\"d-flex align-items-center\">\n                    <img\n                      :src=\"item.img\"\n                      class=\"bg-light rounded-circle p-1 avatar-xs img-fluid\"\n                      alt\n                    />\n                    <h6 class=\"ms-2 mb-0 fs-14\">{{ item.label }}</h6>\n                  </div>\n                  <div class=\"row align-items-end g-0\">\n                    <div class=\"col-6\">\n                      <h5 class=\"mb-1 mt-4\">{{ item.price }}</h5>\n                      <p class=\"text-success fs-13 fw-medium mb-0\">\n                        {{ item.change }}\n                        <span class=\"text-muted ms-2 fs-10 text-uppercase\"\n                          >({{ item.coinName }})</span\n                        >\n                      </p>\n                    </div>\n                    <!-- end col -->\n                    <div class=\"col-6\">\n                      <div class=\"col-6\">\n                      <apexchart\n                        class=\"apex-charts\"\n                        :options=\"item.chartOptions\"\n                        height=\"50\"\n                        dir=\"ltr\"\n                        :series=\"item.series.length > 0 ? item.series : []\"\n                        :key=\"item.id\"\n                      ></apexchart>\n                    </div>\n                    </div>\n                    <!-- end col -->\n                  </div>\n                  <!-- end row -->\n                </div>\n                <!-- end card body -->\n              </div>\n              <!-- end card -->\n            </swiper-slide>\n            <!-- end -->\n          </template>\n        </swiper>\n        <!-- end swiper -->\n      </div>\n      <!-- end col -->\n    </div>\n    <!-- end row -->\n\n    <div class=\"row\">\n      <div class=\"col-xl-8\">\n        <Currencies />\n      </div>\n      <div class=\"col-xl-4\">\n        <Trading />\n      </div>\n    </div>\n\n    <div class=\"row\">\n      <div class=\"col-xxl-4 col-lg-6\">\n        <RecentActivity />\n      </div>\n      <div class=\"col-xxl-4 col-lg-6\">\n        <TopProgram />\n      </div>\n      <div class=\"col-xxl-4 col-lg-6\">\n        <News />\n      </div>\n    </div>\n  </Layout>\n</template>\n"]}]}