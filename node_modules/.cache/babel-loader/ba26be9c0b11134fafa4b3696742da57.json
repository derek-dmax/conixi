ions=\"stackedBarChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!-- end col -->\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"\n              card-header\n              justify-content-between\n              d-flex\n              align-items-center\n            \"\n          >\n            <h4 class=\"card-title\">Stacked Bars 100</h4>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"stackedBar100Chart.series\"\n              :options=\"stackedBar100Chart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!--end col-->\n    </div>\n    <!-- end row -->\n\n    <div class=\"row\">\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"\n              card-header\n              justify-content-between\n              d-flex\n              align-items-center\n            \"\n          >\n            <h4 class=\"card-title\">Bar with Negative Values</h4>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"barNegativeChart.series\"\n              :options=\"barNegativeChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!-- end col -->\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"\n              card-header\n              justify-content-between\n              d-flex\n              align-items-center\n            \"\n          >\n         