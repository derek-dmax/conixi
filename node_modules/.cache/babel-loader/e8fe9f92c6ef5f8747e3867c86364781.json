                </div>\n                <!-- end col -->\n                <div class=\"col-lg-6\">\n                  <div>\n                    <label class=\"form-label mb-0\">DateTime</label>\n                    <p class=\"text-muted\">\n                      Set\n                      <code\n                        >data-provider=\"flatpickr\" data-date-format=\"d.m.y\"\n                        data-enable-time</code\n                      >\n                      attribute.\n                    </p>\n\n                    <flat-pickr\n                      v-model=\"date1\"\n                      :config=\"dateTimeConfig\"\n                      class=\"form-control\"\n                    ></flat-pickr>\n                  </div>\n                </div>\n                <!-- end col -->\n              </div>\n              <!-- end row -->\n              <div class=\"row\">\n                <div class=\"col-lg-6\">\n                  <div class=\"mt-3\">\n                    <label class=\"form-label mb-0\">Human-Friendly Dates</label>\n                    <p class=\"text-muted\">\n                      Set\n                      <code\n                        >data-provider=\"flatpickr\" data-altFormat=\"F j, Y\"</code\n                      >\n                      attribute.\n                    </p>\n                    <flat-pickr\n                      v-model=\"date2\"\n                      :config=\"humanfriendlyConfig\"\n                      class=\"form-control flatpickr-input\"\n                    ></flat-pickr>\n                  </div>\n                </div>\n                <!-- end col -->\n                <div class=\"col-lg-6\">\n                  <div class=\"mt-3\">\n                    <label class=\"form-label mb-0\">MinDate and MaxDate</label>\n                    <p class=\"text-muted\">\n                      Set\n                      <code\n                        >data-provider=\"flatpickr\" data-date-format=\"d M, Y\"\n          