-background\"></div>\n        </div>\n        <!-- Left Sidebar End -->\n        <!-- Vertical Overlay-->\n        <div class=\"vertical-overlay\" id=\"overlay\"></div>\n      </div>\n      <!-- ============================================================== -->\n      <!-- Start Page Content here -->\n      <!-- ============================================================== -->\n  \n      <div class=\"main-content\">\n        <div class=\"page-content\">\n          <!-- Start Content-->\n          <div class=\"container-fluid\">\n            <slot />\n          </div>\n        </div>\n        <Footer />\n      </div>\n      <RightBar />\n    </div>\n  </template>"]}]}