n              </div>\n              <!-- end row -->\n\n              <div class=\"row\">\n                <div class=\"col-lg-6\">\n                  <div class=\"mt-3\">\n                    <label class=\"form-label mb-0\"\n                      >Selecting Multiple Dates</label\n                    >\n                    <p class=\"text-muted\">\n                      Set\n                      <code\n                        >data-provider=\"flatpickr\" data-date-format=\"d M, Y\"\n                        data-multiple-date=\"true\"</code\n                      >\n                      attribute.\n                    </p>\n                    <flat-pickr\n                      v-model=\"date5\"\n                      :config=\"multipleDateConfig\"\n                      class=\"form-control\"\n                    ></flat-pickr>\n                  </div>\n                </div>\n                <!-- end col -->\n                <div class=\"col-lg-6\">\n                  <div class=\"mt-3\">\n                    <label class=\"form-label mb-0\">Range</label>\n                    <p class=\"text-muted\">\n                      Set\n                      <code\n                        >data-provider=\"flatpickr\" data-date-format=\"d M, Y\"\n                        data-range-date=\"true\"</code\n                      >\n                      attribute.\n                    </p>\n                    <flat-pickr\n                      v-model=\"date6\"\n                      :config=\"rangeDateconfig\"\n                      class=\"form-control\"\n                    ></flat-pickr>\n                  </div>\n                </div>\n                <!-- end col -->\n              </div>\n              <!-- end row -->\n\n              <div class=\"row\">\n                <div class=\"col-lg-6\">\n                  <div class=\"mt-3\">\n                    <label class=\"form-label mb-0\">Inline</label>\n                    <p class=\"text-muted\">\n                 