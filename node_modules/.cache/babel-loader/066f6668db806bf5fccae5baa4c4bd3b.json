EAAE,CAAC;MACnBK,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE;QACNC,MAAM,EAAE,GAAG;QACXC,QAAQ,EAAE;MACZ,CAAC;MACDC,MAAM,EAAE,IAAI;MACZC,YAAY,EAAE,CAAC;MACfC,WAAW,EAAE,EAAE;MACfC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,WAAW,EAAE,UAAU;MACvBC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,UAAU,EAAE;IACVlC,MAAM;IACNC,MAAM;IACNC;EACF,CAAC;EACDiC,OAAO,EAAE;IACP;;;IAGAC,aAAa,CAACC,GAAG,EAAE;MACjB,IAAI,CAACpB,eAAe,CAACK,UAAS,GAAIe,GAAG;IACvC,CAAC;IACD;;;IAGAC,SAAS,GAAG;MACV,IAAI,CAACX,MAAK,GAAIY,WAAW,CAAC,MAAM;QAC9B,IAAI,CAACX,YAAY,EAAE;QACnB,IAAI,IAAI,CAACA,YAAW,KAAM,GAAG,EAAE;UAC7BY,aAAa,CAAC,IAAI,CAACb,MAAM,CAAC;QAC5B;MACF,CAAC,EAAE,GAAG,CAAC;IACT;EACF;AACF,CAAC","names":["Slider","Layout","PageHeader","appConfig","page","title","meta","name","content","description","data","items","text","href","active","simpleValue","sliderCustomzie","sliderWithLabel","value","range","label","isHide","rangeValue","merge","format","prefix","decimals","loader","loadingValue","customStyle","stepData","rtlvalue","orientation","direction","components","methods","callbackRange","val","startLoad","setInterval","clearInterval"],"sourceRoot":"","sources":["C:\\Apps\\Velson\\conixi\\src\\views\\forms\\range-sliders.vue"],"sourcesContent":["<script>\n  import Slider from \"@vueform/slider\";\n\n  import Layout from \"../../layouts/main.vue\";\n  import PageHeader from \"@/components/page-header\";\n  import appConfig from \"../../../app.config\";\n\n  export default {\n    page: {\n      title: \"Range Slider\",\n      meta: [{\n        name: \"description\",\n        content: appConfig.description\n      }],\n    },\n    data() {\n      return {\n        title: \"Range Slider\",\n        items: [{\n            text: \"Forms\",\n            href: \"/\",\n          },\n          {\n            text: \"Range Slider\",\n            active: true,\n          },\n        ],\n        simpleValue: 10,\n        sliderCustomzie: 300,\n        sliderWithLabel: {\n          value: 45,\n          data: [15, 30, 45, 60, 75, 90, 120],\n          range: [{\n              label: \"15 mins\",\n            },\n            {\n              label: \"30 mins\",\n              isHide: true,\n            },\n            {\n              label: \"45 mins\",\n            },\n            {\n              label: \"1 hr\",\n              isHide: true,\n            },\n            {\n              label: \"1 hr 15 mins\",\n            },\n            {\n              label: \"1 hr 30 mins\",\n              isHide: true,\n            },\n            {\n              label: \"2 hrs\",\n            },\n          ],\n          rangeValue: {},\n        },\n        value: [20, 30, 40],\n        merge: 10,\n        format: {\n          prefix: \"$\",\n          decimals: 2,\n        },\n        loader: null,\n        loadingValue: 0,\n        customStyle: 50,\n        stepData: 10,\n        rtlvalue: 50,\n        orientation: \"vertical\",\n        direction: \"rtl\",\n      };\n    },\n    components: {\n      Slider,\n      Layout,\n      PageHeader,\n    },\n    methods: {\n      /**\n       * Range and label slider set range\n       */\n      callbackRange(val) {\n        this.sliderWithLabel.rangeValue = val;\n      },\n      /**\n       * Loading slider\n       */\n      startLoad() {\n        this.loader = setInterval(() => {\n          this.loadingValue++;\n          if (this.loadingValue === 100) {\n            clearInterval(this.loader);\n          }\n        }, 100);\n      },\n    },\n  };\n</script>\n\n<template>\n  <Layout>\n    <PageHeader :title=\"title\" :items=\"items\" />\n    <div class=\"row\">\n      <div class=\"col-12\">\n        <div class=\"card\">\n          <div class=\"card-body\">\n            <h4 class=\"card-title\">Range slider</h4>\n            <p class=\"card-title-desc\">\n              Cool, comfortable, responsive and easily customizable range slider\n            </p>\n            <div class=\"row\">\n              <div class=\"col-md-6\">\n                <div class=\"p-3\">\n                  <h5 class=\"font-size-14 mb-5 mt-0\">Default</h5>\n                  <div class=\"p-3\">\n                    <Slider v-model=\"simpleValue\" />\n                  </div>\n                </div>\n              </div>\n              <div class=\"col-md-6\">\n                <div class=\"p-3\">\n                  <h5 class=\"font-size-14 mb-5 mt-0\">Min-Max</h5>\n                  <div class=\"p-3\">\n                    <Slider v-model=\"sliderCustomzie\" :min=\"100\" :max=\"500\" />\n                  </div>\n                </div>\n              </div>\n            </div>\n            <div class=\"row\">\n              <div class=\"col-md-6\">\n                <div class=\"p-3\">\n                  <h5 class=\"font-size-14 mb-5 mt-0\">Range and Label</h5>\n                  <div class=\"p-3\">\n                    <Slider v-model=\"sliderWithLabel.value\" :data=\"sliderWithLabel.data\" :range=\"sliderWithLabel.range\"\n                      @callbackRange=\"callbackRange\" />\n                  </div>\n                </div>\n              </div>\n              <div class=\"col-md-6\">\n                <div class=\"p-3\">\n                  <h5 class=\"font-size-14 mb-5 mt-0\">Loading</h5>\n                  <div class=\"p-3\">\n                    <Slider v-model=\"loadingValue\" :is-disabled=\"true\" class=\"slider-red\" />\n                    <br />\n                    <b-button size=\"sm\" variant=\"light\" @click=\"startLoad\">Start</b-button>\n                  </div>\n                </div>\n              </div>\n            </div>\n            <div class=\"row\">\n              <div class=\"col-md-6\">\n                <div class=\"p-3\">\n                  <h5 class=\"font-size-14 mb-4 mt-0\">Rtl</h5>\n                  <div class=\"p-3\">\n                    <Slider v-model=\"rtlvalue\" :direction=\"direction\" :orientation=\"orientation\" />\n                  </div>\n                </div>\n              </div>\n              <div class=\"col-md-6\">\n                <div class=\"p-3\">\n                  <h5 class=\"font-size-14 mb-5 mt-0\">Format</h5>\n                  <div class=\"p-3\">\n                    <Slider v-model=\"value\" :merge=\"merge\" :format=\"format\" />\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </Layout>\n</template>"]}]}