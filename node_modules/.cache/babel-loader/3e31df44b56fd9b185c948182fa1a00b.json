@/components/page-header\";\r\nimport appConfig from \"../../../../app.config\";\r\nimport Vue3ChartJs from \"@j-t-mcc/vue3-chartjs\";\r\n\r\nimport { line, pie, Doughnut, radar, bar, PolarArea } from \"./data\";\r\n\r\n/**\r\n * Chartjs-chart component\r\n */\r\nexport default {\r\n  page: {\r\n    title: \"Chartjs Charts\",\r\n    meta: [{ name: \"description\", content: appConfig.description }],\r\n  },\r\n  components: {\r\n    Layout,\r\n    PageHeader,\r\n    Vue3ChartJs,\r\n  },\r\n  data() {\r\n    return {\r\n      line: line,\r\n      pie: pie,\r\n      Doughnut: Doughnut,\r\n      radar: radar,\r\n      bar: bar,\r\n      PolarArea: PolarArea,\r\n      title: \"Chartjs\",\r\n      items: [\r\n        {\r\n          text: \"Charts\",\r\n          href: \"/\",\r\n        },\r\n        {\r\n          text: \"Chartjs\",\r\n          active: true,\r\n        },\r\n      ],\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<template>\r\n  <Layout>\r\n    <PageHeader :title=\"title\" :items=\"items\" />\r\n    <div class=\"row\">\r\n      <div class=\"col-lg-6\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h4 class=\"card-title mb-4\">Line Chart</h4>\r\n            <!-- Line Chart -->\r\n            <vue3-chart-js\r\n              class=\"chartjs-chart\"\r\n              v-bind=\"{ ...line }\"\r\n            ></vue3-chart-js>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"col-lg-6\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h4 class=\"card-title mb-4\">Bar Chart</h4>\r\n          \r\n            <!-- Bar Chart -->\r\n            <vue3-chart-js\r\n               class=\"chartjs-chart\"\r\n              v-bind=\"{ ...bar }\"\r\n            ></vue3-chart-js>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"row\">\r\n      <div class=\"col-lg-6\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h4 class=\"card-title mb-4\">Pie Chart</h4>\r\n            \r\n            <!-- Pie Chart -->\r\n             <vue3-chart-js\r\n              :height=\"140\"\r\n              class=\"chartjs-chart\"\r\n              v-bind=\"{ ...pie }\"\r\n            ></vue3-chart-js>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"col-lg-6\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h4 class=\"card-title mb-4\">Donut Chart</h4>\r\n            \r\n            <!-- Donut Chart -->\r\n            <vue3-chart-js\r\n              :height=\"140\"\r\n              class=\"chartjs-chart\"\r\n              v-bind=\"{ ...Doughnut }\"\r\n            ></vue3-chart-js>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"row\">\r\n      <div class=\"col-lg-6\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h4 class=\"card-title mb-4\">Polar area Chart</h4>\r\n            \r\n            <!-- Polar area Chart -->\r\n            <vue3-chart-js\r\n              :height=\"140\"\r\n              class=\"chartjs-chart\"\r\n              v-bind=\"{ ...PolarArea }\"\r\n            ></vue3-chart-js>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"col-lg-6\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <h4 class=\"card-title mb-4\">Radar Chart</h4>\r\n            \r\n            <!-- Radar Chart -->\r\n             <vue3-chart-js\r\n              :height=\"140\"\r\n              class=\"chartjs-chart\"\r\n              v-bind=\"{ ...radar }\"\r\n            ></vue3-chart-js>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </Layout>\r\n</template>"]}]}